listen 80;

index index.html index.htm index.php;

error_page 404 403 500 502 503 504 /404.html;

location / {
    try_files $uri @frp;
}

location @frp {
    proxy_intercept_errors on;
    proxy_pass http://127.0.0.1:85;
    include /data/configs/nginx/nginx_proxy.conf.public;
    expires 1h;
    access_log off;
}
